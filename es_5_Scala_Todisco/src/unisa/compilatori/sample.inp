proc lollo(result: integer, d: integer, b:real, out size: string):

     result  ^=  d;

    var prova : string;\

    if result > 100 then
        var valore ^= "grande";\
         size ^= valore;
    elseif result > 50 then
        var valore ^= "media";\
         size ^= valore;
    else
        var valore ^= "piccola";\
         size ^= valore;
    endif;

endproc

